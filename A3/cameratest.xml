<scenefile>
    <globaldata>
	<diffusecoeff v="0.5"/>
	<specularcoeff v="0.5"/>
	<ambientcoeff v="0.5"/>
    </globaldata>

    <cameradata>
	<pos x="2" y="2" z="2"/>
	<focus x="0" y="0" z="0"/>
	<up x="0" y="1" z="0"/>
	<heightangle v="45"/>
    </cameradata>

    <lightdata>
	<id v="0"/>
	<color r="1" g="1" b="1"/>
	<position x="-4" y="4" z="4"/>
    </lightdata>

    <lightdata>
	<id v="1"/>
	<color r="1" g="1" b="1"/>
	<position x="5" y="4" z="-4"/>
    </lightdata>

    <lightdata>
	<id v="2"/>
	<color r="0.3" g="0.3" b="0.3"/>
	<position x="5" y="1" z="0"/>
    </lightdata>

    <object type="tree" name="root">
    <!-- Base -->
	<transblock>
	    <scale x="1.7" y="1.0" z=".25"/>
	    <translate x="0" y="0" z="0"/>
	    <object type="primitive" name="cube" >
		<diffuse r="0" g="0" b=".5"/>
	    </object>
	</transblock>
	<transblock>
	    <scale x="1.0" y="1.03" z=".25"/>
	    <translate x=".30" y="0.15" z="0"/>
	    <object type="primitive" name="cube" >
		<diffuse r="0" g="0" b=".5"/>
	    </object>
	</transblock>
	<!-- ViewPort -->
	<transblock>
	    <scale x="0.3" y="0.2" z="0.27"/>
	    <translate x="2" y="2.65" z="0"/>
	    <object type="primitive" name="cube" >
		<diffuse r="1" g="0" b="0"/>
	    </object>
	    <object type="tree">
		    <transblock>
			    <scale x="0.8" y="0.8" z="1.05"/>
			    <object type="primitive" name="cube" >
				<diffuse r="1" g="1" b="0"/>
			    </object>
			</transblock>
		</object>
	</transblock>
	<!-- Cover -->
	<transblock>
	    <scale x="1.71" y="0.8" z=".26"/>
	    <translate x="0" y="0" z="0"/>
	    <object type="primitive" name="cube" >
		<diffuse r=".3" g="0.3" b=".3"/>
	    </object>
	</transblock>
	<!-- Lens -->
	<transblock>
	    <scale x="0.7" y="0.7" z="0.1"/>
	    <rotate x="1.0" y="0.0" z="0.0" angle="90" />
	    <translate x="0.5" y="1" z="0"/>
	    <object type="primitive" name="cylinder" >
		<diffuse r="1" g="0" b=".5"/>
	    </object>
	    <object type="tree">
	    	<transblock>
	    		<scale x="0.95" y="0.4" z="0.95"/>
			    <translate x="0" y="3" z="0"/>
			    <object type="primitive" name="cylinder" >
				<diffuse r="1" g="0" b="0"/>
			    </object>
			    <object type="tree">
			    	<transblock>
			    		<scale x="0.90" y="0.4" z="0.90"/>
					    <translate x="0" y="3" z="0"/>
					    <object type="primitive" name="cylinder" >
						<diffuse r="0" g="1" b="0"/>
					    </object>
					    <object type="tree">
					    	<transblock>
					    		<scale x="0.90" y="0.4" z="0.90"/>
							    <translate x="0" y="3" z="0"/>
							    <object type="primitive" name="cylinder" >
								<diffuse r="1" g="1" b="0"/>
							    </object>
							</transblock>
					    </object>
					</transblock>
			    </object>
			</transblock>
	    </object>
	</transblock>
	<!-- Knobs -->
	<transblock>
	    <scale x="0.23" y="0.1" z="0.23"/>
	    <translate x="-2.9" y="5.2" z="0"/>
	    <object type="primitive" name="cylinder" >
		<diffuse r="1" g="0" b=".5"/>
	    </object>
	</transblock>
	<transblock>
	    <scale x="0.23" y="0.1" z="0.23"/>
	    <translate x="-1.7" y="5.2" z="0"/>
	    <object type="primitive" name="cylinder" >
		<diffuse r="1" g="0" b=".5"/>
	    </object>
	</transblock>
    </object>

</scenefile>

